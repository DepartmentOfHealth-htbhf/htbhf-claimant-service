<FindBugsFilter>
    <!-- See example at http://findbugs.sourceforge.net/manual/filter.html#d0e2318 -->
    <Match>
        <Class name="com.foobar.ClassNotToBeAnalyzed" />
    </Match>

    <Match>
        <Class name="com.foobar.ClassWithSomeBugsMatched" />
        <Bug code="DE,UrF,SIC" />
    </Match>

    <Match>
        <Bug code="NP" />
    </Match>
</FindBugsFilter>